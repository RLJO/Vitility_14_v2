<?xml version="1.0" encoding="utf-8" ?>
<odoo>
  
        <template id="All_snippet_options" inherit_id="website.snippet_options">
       

            <xpath expr="//div[@data-js='background']" position="after">
                 
            
                <div data-js="js_embed_html" data-selector=".custom-snippet-embed"></div>
              
            </xpath>
      </template>
	   
   
      <template id="assets_frontend_js" name="Frontend assets" inherit_id="website.assets_frontend">
          <xpath expr="//script[last()]" position="after">
              <script type="text/javascript" src="/theme_impacto/static/src/js/theme_snippets_editor.js"></script>
          </xpath>
      </template>
    
</odoo>