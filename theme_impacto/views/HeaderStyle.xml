<?xml version="1.0" encoding="utf-8"?>
<odoo>

<!-- Header Transprent Light -->
<template id="header_style_2" active="True" inherit_id="website.layout">
    
	   <xpath expr="//div[@id='top_menu_collapse']" position="after">
					<div class="header-icons d-lg-flex align-items-center hide-mobile">
            	<div class="header-setting">
                    <t t-esc="current_language" />
                    <div class="form-language hed-drop">
                        <h4 t-if="(request.is_frontend_multilang)">Language</h4>
                        <ul id="lang" t-if="(request.is_frontend_multilang and len(languages) &gt; 1) or editable" class="js_language_selector">
                            <li>
                                
                                 <button class="btn btn-sm btn-secondary dropdown-toggle" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
										<span t-esc="list(filter(lambda lg : lg[0] == lang, languages))[0][2].split('/').pop()"/>
										<span class="caret ml4"/>
								</button>
                                <ul role="menu" class="dropdown-menu dropdown-menu-right js_language_selector">
									<t t-foreach="languages" t-as="lg">
										<a t-att-href="url_for(request.httprequest.path + '?' + keep_query(), lang_code=lg[0])"
										   class="dropdown-item js_change_lang"
										   t-att-data-url_code="lg[1]">
											<t t-esc="lg[2].split('/').pop()"/>
										</a>
									</t>
                                    
                                    <li groups="website.group_website_publisher">
                                        <t t-set="url_return" t-value="url_for('', '[lang]') + '?' + keep_query()" />
                                        <a t-attf-href="/web#action=base.action_view_base_language_install&amp;website_id=#{website.id}&amp;url_return=#{url_return}">
                                            <i class="fa fa-plus-circle" />Add...
                                        </a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>
						<div class="header-search search_expand">
							<i class="fa fa-search"></i>
							<div class="as-search">
								<form action='/shop' method="get">
								  <input type="text" name="search" placeholder="Search" class="form-control" t-att-value="search"/>
								  <button class="btn btn-default oe_search_button"><i class="fa fa-search"></i></button>
								</form>
								<a href='javascript:void(0);' id="search_close" class="search_close"><i class="fa fa-close"></i></a>
							</div>
						</div>
					
           			<div class="login-header">
              			<ul class="list-inline mb-0">
                            <li class="list-inline-item" t-if="not(website.user_id != user_id)"><a href="/web/login" t-if="not(website.user_id != user_id)"><i class="fa fa-key"></i></a></li>
                            <li class="list-inline-item" t-if="website.user_id != user_id"><a href="/my/home" t-if="website.user_id != user_id"><i class="fa fa-user"></i></a></li>
                            <li class="list-inline-item" t-if="website.user_id != user_id"><a t-attf-href="/web/session/logout?redirect=/" t-if="website.user_id != user_id"><i class="fa fa-sign-out"></i></a></li>
                            
                            
                        </ul>
					</div>
                 <t t-set='is_sale_installed'  t-value="1 if env['ir.module.module'].sudo().search([('name', '=', 'website_sale'),('state','=','installed')]) else 0" />             
           	
						<t t-if="is_sale_installed==1">
            		<t t-set='wishcount' t-value="len(request.env['product.wishlist'].current())"/>
						<div class="header-wishlist" id="my_wish">
							<a class="wishlist-icon" href="/shop/wishlist">
								<i class="fa fa-heart"/>
								<sup t-attf-class="my_wish_quantity o_animate_blink badge badge-primary"><t t-esc='wishcount'/></sup>
							</a>
						</div>
					  

					  <t t-set="website_sale_order" t-value="website.sale_get_order()" />
					  <div class="header-cart" id="my_cart">
						<a class="cart-icon" href="/shop/cart">
						  <i class="fa fa-shopping-cart"></i><sup t-attf-class="my_cart_quantity label badge-primary" t-esc="website_sale_order and website_sale_order.cart_quantity or 0" />
						</a>
					  </div>			  
						</t>				
					</div>       
    </xpath>
    
    
     <xpath expr="//ul[@id='top_menu']"  position="attributes">
         <attribute name="class">nav navbar-nav text-right</attribute>
     </xpath>
    <xpath expr="//header/nav[hasclass('navbar-expand-md')]" position="attributes">
      <attribute name="class">navbar navbar-expand-lg navbar-light bg-light header-main</attribute>
    </xpath>
    
 </template>
 
 
