<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_delivery_carrier_form_with_provider_dhl" model="ir.ui.view">
        <field name="model">delivery.carrier</field>
        <field name="inherit_id" ref="base_dhl.view_delivery_carrier_form_with_provider_dhl"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='dhl_parcel_type']" position="before">
                <field name="saturday_delivery"/>
                <field name="evening_delivery"/>
            </xpath>
        </field>
    </record>

</odoo>
