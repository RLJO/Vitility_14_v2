<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <!-- Navbar -->
    <template id="tus_hidden_navbar" inherit_id="theme_impacto.custom_navbar" name="Hidden Navbar">
        <xpath expr="//nav" position="attributes">
            <attribute name="t-if">not any(word in request.httprequest.path for word in request.urls_to_hide_menubar)</attribute>
        </xpath>
    </template>

</odoo>